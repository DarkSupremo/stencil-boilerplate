{
  "name": "stencil-app",
  "version": "1.0.0",
  "description": "Stencil boilerplate",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build-storybook": "build-storybook -s www",
    "build": "stencil build --stats --prerender",
    "deploy": "gh-pages -d www",
    "dev": "stencil build --dev --watch --serve",
    "lint": "sd concurrent \"npm run lint.ts\" \"npm run lint.styles\"",
    "lint.styles": "stylelint \"src/**/*.css\"",
    "lint.ts": "tslint -p tsconfig.json -c tslint.json",
    "generate-addon-jest-testresults": "jest --config=tests/addon-jest.config.json --json --outputFile=stories/addon-jest.testresults.json",
    "serve": "stencil-dev-server",
    "serve.prod": "npm run build && serve ./www",
    "start": "sd concurrent \"npm run dev\" \"npm run storybook\"",
    "storybook": "start-storybook -p 9006 -s www",
    "stylelint:fix": "stylelint --fix \"src/**/*.css\"",
    "test": "stencil test --spec --e2e --headless",
    "test.ci": "stencil test --spec --e2e --ci",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "test.unit": "stencil test --spec",
    "test.e2e": "stencil test --e2e --headless",
    "test.screenshot": "stencil test --e2e --screenshot"
  },
  "author": "Jag Reehal",
  "license": "ISC",
  "dependencies": {
    "@stencil/core": "^0.18.1"
  },
  "devDependencies": {
    "@stencil/postcss": "^0.1.0",
    "@stencil/utils": "^0.0.5",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-backgrounds": "^5.0.11",
    "@storybook/addon-centered": "^5.0.11",
    "@storybook/addon-events": "^5.0.11",
    "@storybook/addon-jest": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/addon-options": "^5.0.11",
    "@storybook/addon-storyshots": "^5.0.11",
    "@storybook/addon-storysource": "^5.0.11",
    "@storybook/addon-viewport": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/core": "^5.0.11",
    "@storybook/core-events": "5.0.11",
    "@storybook/html": "5.0.11",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/puppeteer": "^1.12.4",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-runtime": "^6.26.0",
    "eventemitter3": "^3.1.2",
    "format-json": "^1.0.3",
    "gh-pages": "2.0.1",
    "global": "^4.3.2",
    "husky": "^2.3.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "pixelmatch": "4.0.2",
    "postcss-custom-media": "^7.0.8",
    "postcss-import-url": "^4.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-reporter": "^6.0.1",
    "prettier": "^1.17.1",
    "puppeteer": "1.16.0",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
